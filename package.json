{
  "name": "yoastseo",
  "description": "Yoast clientside page analysis",
  "homepage": "https://github.com/Yoast/",
  "keywords": [
    "Yoast",
    "SEO",
    "text analysis"
  ],
  "main": "index.js",
  "license": "GPL-3.0",
  "version": "1.28.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoast/YoastSEO.js"
  },
  "scripts": {
    "build": "grunt build:js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "postlint": "grunt check",
    "pretest": "grunt build:test",
    "test": "jest"
  },
  "browser": "js/browser.js",
  "devDependencies": {
    "@types/jest": "^21.1.3",
    "@types/node": "^9.3.0",
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.16.0",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "grunt-babel": "^7.0.0",
    "babelify": "^8.0.0",
    "console.table": "^0.9.1",
    "eslint-config-yoast": "^2.0.2",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-yoast": "^1.0.1",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-exorcise": "^2.1.1",
    "grunt-po2json": "git+https://github.com/atimmer/grunt-po2json.git#f26c05653af89ef0f7a72456dbe141eb56bf5471",
    "grunt-postcss": "^0.8.0",
    "grunt-sass": "^2.0.0",
    "grunt-shell": "^2.0.0",
    "grunt-ts": "^5.5.1",
    "grunt-tslint": "^5.0.1",
    "istanbul": "^0.4.0",
    "jasmine": "~2.5.1",
    "load-grunt-config": "^0.19.1",
    "lodash-cli": "^4.14.1",
    "ts-jest": "^21.1.2",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2"
  },
  "bugs": {
    "url": "https://github.com/Yoast/js-text-analysis/issues"
  },
  "dependencies": {
    "htmlparser2": "^3.9.2",
    "jed": "^1.1.0",
    "jest": "^21.2.1",
    "lodash": "^4.14.1",
    "node-sass": "^4.5.3",
    "sassdash": "^0.8.1",
    "tokenizer2": "^2.0.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/spec/.*\\.(ts|tsx|js)$",
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/spec/helpers/factory.js",
      "/spec/specHelpers/paperChanger.js"
    ],
    "coveragePathIgnorePatterns": [
      "js/templates.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 76,
        "functions": 77,
        "lines": 85,
        "statements": 85
      }
    }
  }
}
